{
  "editor.detectIndentation": false,
  "editor.formatOnSave": true,
  "editor.renderWhitespace": "boundary",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "files.insertFinalNewline": true,
  "files.autoSave": "onFocusChange",
  "quickCommandButtons.buttons": [
    {
      "color": "green",
      "name": "$(git-branch) Git",
      "shortcut": "g",
      "group": [
        {
          "command": "git add . && git commit --amend --no-edit && git push -f",
          "name": "Push",
          "shortcut": "p",
          "terminalName": "Forced Push"
        },
        {
          "command": "git pull",
          "name": "Pull",
          "shortcut": "l",
          "terminalName": "Pull"
        },
        {
          "command": "git add . && git commit --amend --no-edit",
          "name": "Commit",
          "shortcut": "c",
          "terminalName": "Add and Commit Amend"
        },
        {
          "command": "git reset --soft HEAD~1",
          "name": "Reset Soft",
          "shortcut": "r",
          "terminalName": "Reset Soft"
        },
        {
          "command": "git rebase -i HEAD~5",
          "name": "Rebase 5",
          "shortcut": "b",
          "terminalName": "Rebase Interactive 5"
        },
        {
          "command": "git checkout main",
          "name": "Checkout main",
          "shortcut": "m",
          "terminalName": "Checkout main"
        }
      ]
    },
    {
      "color": "#2196F3",
      "executeAll": false,
      "name": "$(play-circle) Run",
      "shortcut": "r",
      "group": [
        {
          "executeAll": true,
          "name": "All",
          "shortcut": "a",
          "group": [
            {
              "command": "just run web-backend",
              "name": "backend",
              "shortcut": "b",
              "terminalName": "backend"
            },
            {
              "command": "just run web-frontend",
              "name": "frontend",
              "shortcut": "f",
              "terminalName": "frontend"
            },
            {
              "command": "just run analyzer",
              "name": "analyzer",
              "shortcut": "w",
              "terminalName": "analyzer"
            },
            {
              "command": "just run spec-generator-mock",
              "name": "spec-generator-mock",
              "shortcut": "g",
              "terminalName": "spec-generator-mock"
            }
          ]
        },
        {
          "command": "just run web-backend",
          "name": "Backend",
          "shortcut": "b",
          "terminalName": "backend"
        },
        {
          "command": "just run web-frontend",
          "name": "Frontend",
          "shortcut": "f",
          "terminalName": "frontend"
        },
        {
          "command": "just run analyzer",
          "name": "Analyzer",
          "shortcut": "w",
          "terminalName": "analyzer"
        },
        {
          "command": "just run spec-generator",
          "name": "Spec Generator",
          "shortcut": "g",
          "terminalName": "spec-generator"
        },
        {
          "command": "just run spec-generator-mock",
          "name": "Spec Generator Mock",
          "shortcut": "m",
          "terminalName": "spec-generator-mock"
        }
      ]
    },
    {
      "color": "tomato",
      "executeAll": false,
      "name": "$(beaker) Test",
      "shortcut": "t",
      "group": [
        {
          "name": "Web ▼",
          "shortcut": "w",
          "group": [
            {
              "command": "just test web",
              "name": "All",
              "shortcut": "a",
              "terminalName": "test web all"
            },
            {
              "command": "just test web backend",
              "name": "Backend",
              "shortcut": "b",
              "terminalName": "test web backend"
            },
            {
              "command": "just test web frontend",
              "name": "Frontend",
              "shortcut": "f",
              "terminalName": "test web frontend"
            },
            {
              "command": "cd apps/web && just test-e2e",
              "name": "E2E",
              "shortcut": "e",
              "terminalName": "test web e2e"
            }
          ]
        },
        {
          "name": "Worker ▼",
          "shortcut": "k",
          "group": [
            {
              "command": "just test worker",
              "name": "All",
              "shortcut": "a",
              "terminalName": "test worker all"
            },
            {
              "command": "just test worker unit",
              "name": "Unit",
              "shortcut": "u",
              "terminalName": "test worker unit"
            },
            {
              "command": "just test worker integration",
              "name": "Integration",
              "shortcut": "i",
              "terminalName": "test worker integration"
            }
          ]
        },
        {
          "name": "Core ▼",
          "shortcut": "c",
          "group": [
            {
              "command": "just test core",
              "name": "All",
              "shortcut": "a",
              "terminalName": "test core all"
            },
            {
              "command": "just test core unit",
              "name": "Unit",
              "shortcut": "u",
              "terminalName": "test core unit"
            },
            {
              "command": "just test core integration",
              "name": "Integration",
              "shortcut": "i",
              "terminalName": "test core integration"
            }
          ]
        }
      ]
    },
    {
      "color": "#9C27B0",
      "executeAll": false,
      "name": "$(database) DB",
      "shortcut": "d",
      "group": [
        {
          "command": "just migrate",
          "name": "Migrate",
          "shortcut": "m",
          "terminalName": "migrate"
        },
        {
          "command": "cd infra && just makemigration ",
          "insertOnly": true,
          "name": "Makemigration",
          "shortcut": "a",
          "terminalName": "makemigration"
        },
        {
          "command": "cd infra && just reset",
          "name": "Reset",
          "shortcut": "r",
          "terminalName": "reset db"
        },
        {
          "command": "cd infra && just erd",
          "name": "ERD",
          "shortcut": "e",
          "terminalName": "erd"
        }
      ]
    },
    {
      "color": "teal",
      "executeAll": false,
      "name": "$(wrench) Utils",
      "shortcut": "u",
      "group": [
        {
          "command": "just deps",
          "name": "Deps",
          "shortcut": "d",
          "terminalName": "deps"
        },
        {
          "command": "just lint",
          "name": "Lint",
          "shortcut": "l",
          "terminalName": "lint"
        },
        {
          "command": "cd apps/web && just gen-api",
          "name": "Gen API",
          "shortcut": "a",
          "terminalName": "gen-api"
        },
        {
          "command": "cd apps/web && just gen-sqlc",
          "name": "Gen SQLC Web",
          "shortcut": "s",
          "terminalName": "gen-sqlc web"
        },
        {
          "command": "cd apps/worker && just gen-sqlc",
          "name": "Gen SQLC Worker",
          "shortcut": "q",
          "terminalName": "gen-sqlc worker"
        },
        {
          "command": "cd apps/web && just update-core",
          "name": "Update Core Web",
          "shortcut": "w",
          "terminalName": "update-core web"
        },
        {
          "command": "cd apps/worker && just update-core",
          "name": "Update Core Worker",
          "shortcut": "k",
          "terminalName": "update-core worker"
        },
        {
          "command": "cd apps/web && just bootstrap",
          "name": "Bootstrap",
          "shortcut": "b",
          "terminalName": "bootstrap"
        },
        {
          "command": "cd packages/core && just snapshot-update",
          "name": "Snapshot Update",
          "shortcut": "n",
          "terminalName": "snapshot-update"
        },
        {
          "command": "cd infra && just gen-schema-docs",
          "name": "Schema Docs",
          "shortcut": "g",
          "terminalName": "schema-docs"
        },
        {
          "command": "cd apps/worker && just clean-containers",
          "name": "Clean Containers",
          "shortcut": "c",
          "terminalName": "clean-containers"
        }
      ]
    },
    {
      "color": "orange",
      "command": "just release",
      "name": "$(rocket) Release",
      "terminalName": "Release"
    },
    {
      "color": "blue",
      "command": "export IS_SANDBOX=1 && claude --dangerously-skip-permissions",
      "name": "$(hubot) Claude",
      "newTerminal": true,
      "shortcut": "c",
      "terminalName": "Claude Yolo",
      "useVsCodeApi": false
    }
  ],
  "quickCommandButtons.refreshButton": {
    "icon": "$(refresh)",
    "color": "gray",
    "enabled": true
  }
}
